{
  "name": "@muenchen/eappointment-buergeransicht",
  "version": "1.2.8",
  "description": "Frontend client for Appointments system",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "init": "npm install",
    "serve": "vue-cli-service serve --port 8082",
    "build": "vue-cli-service build && npm run post-build",
    "post-build": "node ./processes/post-build.js",
    "lint": "vue-cli-service lint --fix",
    "test:unit": "vue-cli-service test:unit --passWithNoTests",
    "test": "jest js --coverage"
  },
  "dependencies": {
    "@mdi/js": "^6.5.95",
    "@somushq/vue-friendly-captcha": "^1.0.0",
    "@vue/test-utils": "^1.3.5",
    "@vue/vue2-jest": "^27.0.0-alpha.3",
    "@vue/web-component-wrapper": "^1.3.0",
    "axios": "^0.29.0",
    "core-js": "^3.8.3",
    "jest-environment-jsdom": "^27.0.0-alpha.3",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.45",
    "pump": "^3.0.0",
    "regenerator-runtime": "^0.14.1",
    "vue": "^2.6.14",
    "vue-axios": "^3.4.1",
    "vue-i18n": "^8.28.2",
    "vue-router": "^3.6.5",
    "vuelidate": "^0.7.7",
    "vuetify": "^2.6.14",
    "vuex": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "~5.0.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.7.1",
    "jest": "^27.0.0-alpha.3",
    "jest-fetch-mock": "^3.0.3",
    "minimist": "^1.2.5",
    "postcss": "^8.4.6",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-vue": "^5.1.9",
    "sass": "~1.32",
    "sass-loader": "^10.1.1",
    "ttypescript": "^1.5.12",
    "typescript": "^4.0.3",
    "vue": "^2.6.14",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.0",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.2",
    "webpack": "^5.2.0",
    "webpack-assets-manifest": "^5.1.0"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/it-at-m/eappointment-buergeransicht"
  },
  "keywords": [
    "starter",
    "vuetify",
    "typescript",
    "rollup"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/translations",
      "<rootDir>/src/store/modules/api/index.js",
      "<rootDir>/src/store/modules/formData/index.js",
      "<rootDir>/src/store/modules/formData/acti expect(commitMethods.mock.calls[0]).toStrictEqual(['data/setService', ns.js",
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.vue$": "@vue/vue2-jest",
      "^.+\\.js$": "babel-jest"
    }
  }
}
